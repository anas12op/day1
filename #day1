#include <stdio.h>

int main() {
    int a = 10;
    int b = 786;
    a += b; 
    printf("a += b: %d\n", a);

    a -= b; // Equivalent to: a = a - b;
    printf("a -= b: %d\n", a);

    a *= b; // Equivalent to: a = a * b;
    printf("a *= b: %d\n", a);

    a /= b; // Equivalent to: a = a / b;
    printf("a /= b: %d\n", a);

    a %= b; // Equivalent to: a = a % b;
    printf("a %%= b: %d\n", a);

    return 0;
}
